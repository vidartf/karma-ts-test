{
  "name": "karma-ts-test",
  "version": "1.0.0",
  "description": "Test",
  "directories": {
    "lib": "lib/"
  },
  "scripts": {
    "clean": "rimraf node_modules",
    "build": "npm run build:src",
    "build:src": "tsc --project src",
    "build:test": "tsc --project test/src",
    "update-other": "rimraf ./node_modules/test-codemirror && npm install",
    "test": "karma start test/karma.conf.js",
    "test:chrome": "karma start --browsers=Chrome test/karma.conf.js",
    "test:firefox": "karma start --browsers=Firefox test/karma.conf.js",
    "update": "rimraf node_modules/karma-typescript && npm install"
  },
  "author": "",
  "license": "BSD-3-Clause",
  "dependencies": {
    "test-codemirror": "./other_package",
    "codemirror": "^5.21.0"
  },
  "devDependencies": {
    "@types/expect.js": "^0.3.29",
    "@types/mocha": "^2.2.33",
    "@types/node": "7.0.12",
    "expect.js": "^0.3.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-typescript": "git://github.com/monounity/karma-typescript.git",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4",
    "typescript": "~2.2.2"
  }
}
